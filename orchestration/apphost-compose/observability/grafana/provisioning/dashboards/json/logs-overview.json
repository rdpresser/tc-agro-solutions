{
  "uid": "tc-logs",
  "title": "TC Agro - Logs",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "type": "logs",
      "title": "Recent Logs",
      "datasource": {
        "type": "loki",
        "uid": "Loki"
      },
      "gridPos": { "h": 18, "w": 24, "x": 0, "y": 0 },
      "options": {
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": true,
        "showTime": true,
        "wrapLogMessage": true
      },
      "targets": [
        {
          "expr": "{app=~\".*\"}",
          "refId": "A"
        }
      ]
    }
  ],
  "templating": { "list": [] }
}
